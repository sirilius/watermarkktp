<div class="relative bg-white">
  <div class="max-w-7xl mx-auto">
    <div
      class="flex justify-between items-center border-b-2 border-gray-100 py-6 lg:justify-start lg:space-x-10"
    >
      <div class="flex justify-start text-2xl font-bold">
        <a href="/">
          <h1>Watermark KTP</h1>
        </a>
      </div>
      <div class="-mr-2 -my-2 lg:hidden">
        <button
          type="button"
          class="bg-white rounded-md p-2 inline-flex flex-col items-center justify-between h-[29px] text-gray-500 hover:text-gray-500 hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-indigo-500"
          aria-expanded="false"
          onclick="toggle_menu()"
        >
          <span class="sr-only">Open menu</span>
          <span
            class="w-5 h-0.5 bg-gray-400 rounded-lg duration-300"
            id="line-1"
          ></span>
          <span
            class="w-5 h-0.5 bg-gray-400 rounded-lg duration-300"
            id="line-2"
          ></span>
          <span
            class="w-5 h-0.5 bg-gray-400 rounded-lg duration-300"
            id="line-3"
          ></span>
        </button>
      </div>
      
      <nav class="hidden lg:flex space-x-10">
        {% set navPages = collections.all | eleventyNavigation %}
        {%- for entry in navPages %}
          <a
            href="{{ entry.url | url }}"
            class="text-base font-medium text-gray-500 hover:text-gray-900"
            {% if entry.url == page.url %} style="color: #111827"{% endif %}
          >
            {{ entry.title }}
          </a>
        {%- endfor %}

        <a
          href="https://github.com/sirilius/watermarkktp"
          class="text-base font-medium text-gray-500 hover:text-gray-900"
          data-umami-event="top-menu-GitHub"
        >
          GitHub
        </a>

        <a
          href="https://twitter.com/SiriliusKevin"
          class="text-base font-medium text-gray-500 hover:text-gray-900"
          data-umami-event="top-menu-Twitter"
        >
          Twitter
        </a>
      </nav>

      <div class="hidden lg:flex items-center justify-end lg:flex-1 lg:w-0">
        <a
          href="https://saweria.co/sirilius"
          target="_blank"
          class="ml-8 whitespace-nowrap inline-flex items-center justify-center px-4 py-2 border border-transparent rounded-lg text-base font-medium text-white bg-blue-500 text-blue shadow-3d-blue-button bg-gradient-to-b from-gradient-btn-from to-gradient-btn-to bg-no-repeat hover:opacity-80"
          data-umami-event="top-menu-Saweria"
        >
          Support Me
        </a>
      </div>
    </div>
  </div>

  <div
    class="inset-x-0 pt-6 transition transform origin-top-right hidden"
    id="menu"
  >
